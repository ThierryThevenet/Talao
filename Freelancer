pragma solidity ^0.4.0;
contract FreelancerStorage {
  
  struct MemberData {
      uint num;
      uint age;
      uint skill;    }
   
   MemberData [5] Freelancer;
   
   function set(uint n, uint x, uint y , uint z)
      Freelancer[n].num = x;
      Freelancer[n].age=y;
      Freelancer[n].skill=z;    }
    
   function get(uint z) constant returns (uint, uint, uint) { 
      return (Freelancer[z].num, Freelancer[z].age, Freelancer[z].skill);  
    }
 }
